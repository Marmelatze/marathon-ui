{
  "name": "marathon-assets",
  "version": "0.1.0",
  "description": "Static assets for Mesosphere's Marathon.",
  "main": "js/main.js",
  "keywords": [
    "marathon",
    "mesos"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mesosphere/marathon.git"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "flux": "^2.0.3",
    "jquery": "^2.1.3",
    "lazy.js": "^0.4.0",
    "moment": "^2.9.0",
    "mousetrap": "^1.4.6",
    "oboe": "^2.1.2",
    "react": "^0.12.2",
    "react-tools": "^0.12.2",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "broccoli": "^0.13.2",
    "broccoli-asset-rev": "^2.0.0",
    "broccoli-clean-css": "^0.3.0",
    "broccoli-concat": "0.0.12",
    "broccoli-env": "0.0.1",
    "broccoli-export-tree": "^0.3.2",
    "broccoli-jscs": "0.0.13",
    "broccoli-jshint": "^0.5.3",
    "broccoli-less": "^2.0.0",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-react": "^0.7.0",
    "broccoli-replace": "^0.2.0",
    "broccoli-static-compiler": "^0.1.4",
    "broccoli-uglify-js": "^0.1.3",
    "broccoli-webpack": "^0.1.1",
    "chai": "^3.0.0",
    "chalk": "^0.5.1",
    "cheerio": "^0.18.0",
    "jsdom": "^3.1.2",
    "mocha": "^2.1.0",
    "npm-shrinkwrap": "^5.4.0",
    "sinon": "^1.12.2"
  },
  "scripts": {
    "build": "broccoli build dist",
    "clean": "rm -rf dist tmp",
    "dist": "export BROCCOLI_ENV=\"production\" && npm run clean && npm run test && npm run build",
    "test": "mocha --ui exports --reporter spec --compilers jsx:test/helpers/compiler.js test/helpers/object-assign-polyfill.js test/**/*.test.* test/*.test.*",
    "test-watch": "mocha --ui exports --reporter spec --watch --compilers jsx:test/helpers/compiler.js test/helpers/object-assign-polyfill.js test/**/*.test.* test/*.test.*",
    "watch": "export BROCCOLI_ENV=\"development\" && broccoli-timepiece dist",
    "shrinkwrap": "npm-shrinkwrap --dev"
  }
}
